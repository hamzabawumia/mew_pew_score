<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  {% include "sections/CDN_imports.html" %}

</head>
<body class="hold-transition sidebar-mini">
<div class="wrapper">


  {% include "sections/top_navbar.html" %}

  {% include "sections/side_bar.html" %}



<!-- MAIN CONTENTS Content Wrapper. Contains page content -->
<div class="content-wrapper">

    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
          </div><!-- /.col -->
          <div class="col-sm-6">
          </div><!-- /.col -->

        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <div class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-lg-12">
              <div class="card">
                <div class="card-body">
                  <h5 class="card-title"></h5>
  
                  <p class="card-text">

                  {% block content %}

                  {% endblock %}
                  
                  </p>
  
                </div>
              </div>
  

            </div>


          </div>
          <!-- /.row -->
        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content -->
</div>
    <!-- /.content-wrapper -->

<!-- END OF MAIN_CONTENTS AREA -->



  {% include "sections/footer.html" %}
</div>
